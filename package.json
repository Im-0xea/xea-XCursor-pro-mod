{
  "dependencies": {
    "cbmp": "ful1e5/cbmp"
  },
  "scripts": {
    "render:xcursor-pro-dark": "npx cbmp -d 'svg' -n 'XCursor-Pro-Dark' -bc '#000000' -oc '#FFFFFF'",
    "render": "yarn render:xcursor-pro-dark && yarn render:xcursor-pro-light && yarn render:xcursor-pro-red",
